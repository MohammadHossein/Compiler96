صحیح محاسبه_فاکتوریل ( صحیح شماره ) {
	صحیح شمارنده = ۱ ؛
	صحیح نتیجه = ۱ ؛
	اگر شماره == ۰ یا شماره == ۱ آنگاه { برگردان ۱ ؛ }
	وگرنه {
		وقتی (شمارنده <= شماره) {
		نتیجه *= شمارنده ؛
		شمارنده++؛
		}
	}
}

صحیح اصلی () {
